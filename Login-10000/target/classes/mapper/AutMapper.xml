<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.org.login10000.mapper.AutMapper">
<select id="selectOne" resultType="com.org.entity.Aut">
    select * from aut where email  = #{email}
</select>
    <select id="selectOneByOpenid" resultType="com.org.entity.Aut">
        select * from aut where openid  = #{openid}
    </select>
</mapper>
